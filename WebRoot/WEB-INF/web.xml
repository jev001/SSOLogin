<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>SSOLogin</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
   <servlet>
    <servlet-name>IndexService</servlet-name>
    <servlet-class>com.gs.action.IndexService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>IndexService</servlet-name>
    <url-pattern>/IndexService</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>LoginService</servlet-name>
    <servlet-class>com.gs.action.LoginService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LoginService</servlet-name>
    <url-pattern>/LoginService</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>PreLoginService</servlet-name>
    <servlet-class>com.gs.action.PreLoginService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>PreLoginService</servlet-name>
    <url-pattern>/PreLoginService</url-pattern>
  </servlet-mapping>
   <servlet>
    <servlet-name>ValidateService</servlet-name>
    <servlet-class>com.gs.action.ValidateService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ValidateService</servlet-name>
    <url-pattern>/ValidateService</url-pattern>
  </servlet-mapping>
</web-app>